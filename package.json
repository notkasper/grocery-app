{
  "name": "hs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "stop": "docker-compose down",
    "prod": "npm run services:prod",
    "dev": "concurrently \"npm run services:dev\" \"npm run front-end:dev\"",
    "build:front-end": "cd services/front-end && webpack -p --mode production",
    "build:dev": "docker-compose -f docker-compose.dev.yml build",
    "build:prod": "docker-compose -f docker-compose.prod.yml build",
    "services:dev": "docker-compose -f docker-compose.dev.yml up",
    "services:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "front-end:dev": "cd services/front-end && webpack-dev-server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src": [
      "npm run lint"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "prettier-eslint": "^9.0.1"
  }
}
